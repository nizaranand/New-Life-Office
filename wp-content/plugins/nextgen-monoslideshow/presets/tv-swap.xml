<?xml version="1.0" encoding="utf-8"?>

<album>
	<configuration delay="6" viewport="0,0,0,0,offset" position="topRight" >
		<caption backgroundRoundness="2" backgroundType="fitCompleteText" margin="10,3,0,3" padding="5,0,5,5" showCount="false" width="80%" >
			<title color="#ffffff" size="16" weight="bold" />
			<description color="#e13a3e" size="18" weight="bold"/>
			<shadow/>
			<transition direction="topToBottom" />
		</caption>
		<navigation>
			<albums backgroundAlpha="0" backgroundColor="#000000" padding="0,0,0,0" imageScaleMode="downscaleToFit" >
				<caption>
					<title/>
					<description/>
				</caption>
				<shadow/>
				<transition distanceX="10" highlight="screen" highlightStrength="0">
					<shadow/>
				</transition>
			</albums>
			<items imageScaleMode="downscaleToFit">
				<shadow/>
				<transition>
					<shadow/>
				</transition>
			</items>
			<shadow/>
			<caption>
				<title/>
				<description/>
			</caption>
			<pageIndicator/>
		</navigation>
		<controller>
			<transition/>
			<scrubBar/>
			<shadow/>
		</controller>
		<mark>
			<shadow/>
		</mark>
		<progress>
			<shadow/>
		</progress>
		<transition size="0.15" strength="0.75" time="2" type="blend" />
		<shadow/>
		<sound/>
		<thumbnailNavigation backgroundAlpha="0" backgroundRoundness="0" iconHeight="15" iconMargin="3,3,3,3" iconWidth="15" margin="0,10,0,15" padding="0,0,0,0" strokeColor="#333333" type="normal" >
			<items height="50" strokeColor="#000000" width="50" >
				<shadow/>
				<transition scale="2" >
					<shadow sizeX="4" sizeY="4" strength="1" />
				</transition>
			</items>
			<shadow/>
		</thumbnailNavigation>
	</configuration>
	<contents>
		 <!-- ... -->
	</contents>
</album>