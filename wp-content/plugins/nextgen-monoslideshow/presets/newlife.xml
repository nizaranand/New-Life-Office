<?xml version="1.0" encoding="utf-8"?>


<configuration viewport="40,80,40,80,offset" fullScreenScaleMode='downscaleToFit' scaleMode='downscaleToFit' backgroundColor='#ffffff' delay="4" startWithAlbumID="salt_lake" >

<progress />
