</controller>  
<album >
<image />        
<transition easing='easeOut' time="2.5"/>
</album>
<thumbnailNavigation backgroundAlpha=".5" backgroundRoundness="0" iconHeight="45" iconMargin="3,3,3,3" iconWidth="45" margin="0,10,0,15" padding="0,0,0,0" strokeColor="#333333" type="normal" >
